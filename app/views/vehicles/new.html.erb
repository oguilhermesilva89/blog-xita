<h2>New Vehicle</h2>

<%= form_with model: @vehicle, data: { turbo: false } do |f| %>
  <p>
    <%= f.label :brand, 'Brand:' %>
    <%= f.text_field :brand %>
  </p>

  <p>
    <%= f.label :model, 'Model:' %>
    <%= f.text_field :model %>
  </p>

  <p>
  <%= f.label :year, 'Year:' %>
  <%= f.number_field :year %>
  </p> 
 
  <p>
    <%= f.label :kind, 'Kind:' %>
    <%= f.number_field :kind %>
  </p> 

  <p>
    <%= f.submit %>
  </p>

<% end %>